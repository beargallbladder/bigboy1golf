{
  "version": 2,
  "routes": [
    {
      "src": "^/$",
      "dest": "/landing.html"
    },
    {
      "src": "^/app$",
      "dest": "/index.html"
    },
    {
      "src": "^/index.html$",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
} 